generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model posts {
  id      String  @id @default(cuid())
  content String?
  email   String?
}

model users {
  id       String @id @default(cuid())
  email    String @unique
  username String
}
